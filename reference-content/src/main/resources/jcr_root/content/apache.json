			"jcr:content": {
				"jcr:primaryType": "nt:unstructured",
				"jcr:title": "Apache Sling - Bringing Back the Fun!",
				"sling:template": "/conf/global/site/templates/base-page",
				"sling:taxonomy": "/etc/taxonomy/reference/community",
